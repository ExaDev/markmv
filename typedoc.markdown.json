{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": ["typedoc-plugin-markdown"],
  "entryPoints": ["src/index.ts", "src/commands/convert.ts", "src/commands/index.ts"],
  "out": "docs-markdown",
  "name": "markmv",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": false,
  "excludeNotDocumented": true,
  "readme": "templates/README-for-typedoc.md",
  "gitRevision": "main",
  "gitRemote": "origin",
  "sourceLinkTemplate": "https://github.com/ExaDev/markmv/blob/{gitRevision}/{path}#L{line}",
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Core API",
    "Commands", 
    "Strategies",
    "Types",
    "Utilities",
    "*"
  ],
  "sort": ["source-order"],
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": true
  },
  "treatWarningsAsErrors": false,
  "logLevel": "Info",
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/cli.ts",
    "**/mcp-server.ts",
    "**/api-server.ts",
    "node_modules/**/*"
  ],
  "externalPattern": [
    "**/node_modules/**"
  ],
  "outputFileStrategy": "modules",
  "mergeReadme": true,
  "hidePageHeader": true,
  "hideBreadcrumbs": true,
  "hidePageTitle": true,
  "useCodeBlocks": true,
  "expandParameters": true,
  "outputFileStrategy": "modules",
  "flattenOutputFiles": true,
  "titleLink": "https://github.com/ExaDev/markmv",
  "navigationLinks": {
    "GitHub": "https://github.com/ExaDev/markmv",
    "npm": "https://www.npmjs.com/package/markmv"
  },
  "sidebarLinks": {
    "Documentation": "https://exadev.github.io/markmv/",
    "Changelog": "https://github.com/ExaDev/markmv/blob/main/CHANGELOG.md"
  }
}